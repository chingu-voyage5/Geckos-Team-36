{
    “manifest_version”: 2,
    “name”: “HackerTab”,
    “description”: "HackerNew in your chrome new tab”,
    “version”: “1”,
    “author”: “Dinesh Thapa Magar”,
   "browser_action": {
      "default_icon": "tab-icon.png",
      “default_title”: “Hacker News”
    },
   “chrome_url_overrides” : {
     “newtab”: “newtab.html”
   },
    “permissions”: [“activeTab”]
   }